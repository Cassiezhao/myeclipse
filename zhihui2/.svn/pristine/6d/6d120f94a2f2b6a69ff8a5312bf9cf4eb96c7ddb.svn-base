			document.form1.rows.value=${pathpb.rows};
			return;
		}
		window.location="../findByGid_Paths?index=${sessions.guide.guideid }&rows="+rows;
	}
	function dopage(){
		var page=document.form1.page.value;
		if(isNaN(page)){
