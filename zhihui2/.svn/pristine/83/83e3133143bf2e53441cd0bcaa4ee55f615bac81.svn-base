										+ pageNumber + "&rows=" + pageSize;
							}
						});
			</script>
			<script>
				function dorows() {
					var rows = document.form1.rows.value;
					if (isNaN(rows)) {
						alert("请输入正确的数字！");
						document.form1.rows.value = $
						{
							pb2.rows
						}
						;
						return;
					}
					window.location = "../findAll_Guidem?i=1&rows=" + rows;
				}
				function dopage() {
					var page = document.form1.page.value;
					if (isNaN(page)) {
						alert("请输入正确的数字！");
						document.form1.page.value = $
						{
							pb2.page
						}
