				$("#myid2").attr('value',num);
				$('#return').dialog({
					title : '完整归还导游机',
					width : 400,
					height : 200,
					closed : false,
					iconCls:'icon-save',
					cache : true,
					href : '',
					modal : true
				});
				}
			/*归还按钮 end*/	
				
				
				$('#pp').pagination(
